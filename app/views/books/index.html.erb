<h1>Welcome</h1>
<h1>Books#index</h1>

<table>
  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= link_to "#{book.name}",book_path(id: book.id)%></td>
    </tr>
    <% end %>
  </tbody>
</table>


<%= form_tag("/books", class: "") do %>
  <%= text_field_tag "book[name]" %>
  <%= submit_tag %>
<% end %>
